{
  "name": "root",
  "private": true,
  "description": "Edifice UI Frontend Library",
  "homepage": "https://github.com/opendigitaleducation/edifice-ui#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/edifice-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendigitaleducation/edifice-ui.git"
  },
  "license": "AGPL-3.0",
  "author": "Edifice",
  "type": "module",
  "scripts": {
    "build": "pnpm dlx nx run-many -t build --output-style=stream",
    "build:bootstrap": "pnpm dlx nx build bootstrap",
    "build:client": "pnpm dlx nx build client",
    "build:icons": "pnpm dlx nx build icons",
    "build:react": "pnpm dlx nx build react",
    "build:titpap-extensions": "pnpm dlx nx build titpap-extensions",
    "lint": "pnpm dlx nx run-many -t lint --output-style=stream",
    "lint:bootstrap": "pnpm dlx nx lint bootstrap",
    "lint:client": "pnpm dlx nx lint client",
    "lint:icons": "pnpm dlx nx lint icons",
    "lint:react": "pnpm dlx nx lint react",
    "lint:titpap-extensions": "pnpm dlx nx lint titpap-extensions",
    "check:bootstrap": "package-check --cwd=packages/bootstrap",
    "check:client": "package-check --cwd=packages/client",
    "check:icons": "package-check --cwd=packages/icons",
    "check:react": "package-check --cwd=packages/react",
    "check:tiptap-extensions": "package-check --cwd=packages/tiptap/extensions",
    "clean": "rm -rf */**/node_modules && rm -rf node_modules && rm -f pnpm-lock.yaml && rm -rf */**/dist && rm -rf packages/**/.turbo",
    "clean:dist": "rm -rf */**/dist",
    "clean:lock": "rm -f pnpm-lock.yaml",
    "clean:modules": "rm -rf */**/node_modules",
    "clean:storybook": "rm -rf dist",
    "docs": "storybook dev -p 6006",
    "docs:build": "storybook build --docs -o dist/",
    "docs:preview": "npx http-server ./dist",
    "preinstall": "npx only-allow pnpm",
    "pre-commit": "pnpm run lint && pnpm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-pure-annotations": "7.23.3",
    "@edifice.io/icons": "workspace:*",
    "@edifice.io/react": "workspace:*",
    "@nx/eslint": "19.8.0",
    "@nx/eslint-plugin": "19.8.0",
    "@nx/storybook": "19.8.0",
    "@nx/vite": "19.8.0",
    "@nx/js": "19.8.0",
    "@skypack/package-check": "0.2.2",
    "@storybook/addon-a11y": "7.6.6",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/blocks": "7.6.6",
    "@storybook/manager-api": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/react-vite": "7.6.6",
    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "7.6.6",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.7.26",
    "@swc/cli": "~0.3.12",
    "@swc/helpers": "~0.5.11",
    "@vitejs/plugin-react": "4.3.1",
    "@vitejs/plugin-react-swc": "3.7.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-storybook": "0.6.15",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "nx": "19.8.0",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@testing-library/react": "15.0.6",
    "@types/jest": "29.4.0",
    "@types/node": "18.16.9",
    "@types/pako": "2.0.3",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "rollup-plugin-pure": "^0.2.1",
    "rollup-plugin-visualizer": "5.12.0",
    "storybook": "7.6.6",
    "turbo": "2.0.14",
    "typescript": "5.3.3",
    "vite": "5.4.2",
    "vite-plugin-dts": "4.0.3",
    "@vitest/coverage-v8": "1.0.4",
    "@vitest/ui": "1.3.1",
    "babel-jest": "29.4.1",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jsdom": "~22.1.0",
    "msw": "2.0.11",
    "msw-storybook-addon": "2.0.0-beta.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "vitest": "1.3.1",
    "@typescript-eslint/eslint-plugin": "7.3.0",
    "@typescript-eslint/parser": "7.3.0"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": "18 || 20"
  }
}