{
  "name": "@edifice.io/tiptap-extensions",
  "version": "2.5.9",
  "description": "Edifice Rich Text Editor Extensions",
  "keywords": [
    "edifice",
    "rich text editor",
    "tiptap",
    "extensions"
  ],
  "homepage": "https://github.com/edificeio/edifice-frontend-framework/tree/main/packages/extensions#readme",
  "bugs": {
    "url": "https://github.com/edificeio/edifice-frontend-framework/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edificeio/edifice-frontend-framework.git",
    "directory": "packages/extensions"
  },
  "license": "AGPL-3.0",
  "author": "Edifice",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./abbr": {
      "import": "./dist/abbr/abbr.js",
      "require": "./dist/abbr/abbr.cjs"
    },
    "./alert": {
      "import": "./dist/alert/alert.js",
      "require": "./dist/alert/alert.cjs"
    },
    "./attachment": {
      "import": "./dist/attachment/attachment.js",
      "require": "./dist/attachment/attachment.cjs"
    },
    "./audio": {
      "import": "./dist/audio/audio.js",
      "require": "./dist/audio/audio.cjs"
    },
    "./conversation-history": {
      "import": "./dist/conversation-history/conversation-history.js",
      "require": "./dist/conversation-history/conversation-history.cjs"
    },
    "./conversation-history-body": {
      "import": "./dist/conversation-history-body/conversation-history-body.js",
      "require": "./dist/conversation-history-body/conversation-history-body.cjs"
    },
    "./font-size": {
      "import": "./dist/font-size/font-size.js",
      "require": "./dist/font-size/font-size.cjs"
    },
    "./heading": {
      "import": "./dist/heading/heading.js",
      "require": "./dist/heading/heading.cjs"
    },
    "./highlight": {
      "import": "./dist/highlight/highlight.js",
      "require": "./dist/highlight/highlight.cjs"
    },
    "./hyperlink": {
      "import": "./dist/hyperlink/hyperlink.js",
      "require": "./dist/hyperlink/hyperlink.cjs"
    },
    "./iframe": {
      "import": "./dist/iframe/iframe.js",
      "require": "./dist/iframe/iframe.cjs"
    },
    "./image": {
      "import": "./dist/image/image.js",
      "require": "./dist/image/image.cjs"
    },
    "./line-height": {
      "import": "./dist/line-height/line-height.js",
      "require": "./dist/line-height/line-height.cjs"
    },
    "./linker": {
      "import": "./dist/linker/linker.js",
      "require": "./dist/linker/linker.cjs"
    },
    "./mathjax": {
      "import": "./dist/mathjax/mathjax.js",
      "require": "./dist/mathjax/mathjax.cjs"
    },
    "./paragraph": {
      "import": "./dist/paragraph/paragraph.js",
      "require": "./dist/paragraph/paragraph.cjs"
    },
    "./speech-recognition": {
      "import": "./dist/speech-recognition/speech-recognition.js",
      "require": "./dist/speech-recognition/speech-recognition.cjs"
    },
    "./speech-synthesis": {
      "import": "./dist/speech-synthesis/speech-synthesis.js",
      "require": "./dist/speech-synthesis/speech-synthesis.cjs"
    },
    "./table-cell": {
      "import": "./dist/table-cell/table-cell.js",
      "require": "./dist/table-cell/table-cell.cjs"
    },
    "./transform": {
      "import": "./dist/transform/transform.js",
      "require": "./dist/transform/transform.cjs"
    },
    "./video": {
      "import": "./dist/video/video.js",
      "require": "./dist/video/video.cjs"
    },
    "./information-pane": {
      "import": "./dist/information-pane/information-pane.js",
      "require": "./dist/information-pane/information-pane.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "fix": "eslint --fix --report-unused-disable-directives --max-warnings 0",
    "format": "pnpm run format:write && pnpm run format:check",
    "format:check": "npx prettier --check \"src/**/*.ts\"",
    "format:write": "npx prettier --write \"src/**/*.ts\"",
    "lint": "eslint"
  },
  "dependencies": {
    "@edifice.io/utilities": "workspace:*",
    "@tiptap/core": "3.10.2",
    "@tiptap/extension-blockquote": "3.10.2",
    "@tiptap/extension-bold": "3.10.2",
    "@tiptap/extension-list": "3.10.2",
    "@tiptap/extension-code": "3.10.2",
    "@tiptap/extension-code-block": "3.10.2",
    "@tiptap/extension-document": "3.10.2",
    "@tiptap/extension-hard-break": "3.10.2",
    "@tiptap/extension-heading": "3.10.2",
    "@tiptap/extension-highlight": "3.10.2",
    "@tiptap/extension-horizontal-rule": "3.10.2",
    "@tiptap/extension-image": "3.10.2",
    "@tiptap/extension-italic": "3.10.2",
    "@tiptap/extension-link": "3.10.2",
    "@tiptap/extension-paragraph": "3.10.2",
    "@tiptap/extension-strike": "3.10.2",
    "@tiptap/extension-table": "3.10.2",
    "@tiptap/extension-text": "3.10.2",
    "@tiptap/extension-text-style": "3.10.2",
    "@tiptap/extensions": "^3.10.2",
    "@tiptap/html": "3.10.2",
    "@tiptap/pm": "3.10.2",
    "@tiptap/starter-kit": "3.10.2",
    "prosemirror-model": "^1.0.0",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.0.0",
    "prosemirror-view": "^1.27.0",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "devDependencies": {
    "@edifice.io/client": "workspace:*",
    "@types/dom-speech-recognition": "^0.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
